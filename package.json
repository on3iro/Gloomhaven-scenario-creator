{
  "name": "gloomhaven-scenario-creator",
  "version": "1.0.0",
  "description": "This is a react web frontend for passCreator",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node server",
    "build": "rimraf dist && webpack --config ./webpack/webpack.production.config.js --progress --profile --colors",
    "lint": "eslint ./",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "coveralls": "jest --coverage --runInBand && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/on3iro/Gloomhaven-scenario-creator.git"
  },
  "keywords": [
    "frontend",
    "gloomhaven",
    "react",
    "redux"
  ],
  "author": "Theo Salzmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/on3iro/Gloomhaven-scenario-creator/issues"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "modulePaths": [
      "src"
    ],
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}",
      "!**/src/global-styles.js",
      "!**/src/index.js",
      "!**/src/routes.js",
      "!**/src/store.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ]
  },
  "homepage": "https://github.com/on3iro/passCreator-Frontend#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel": "^6.5.2",
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "coveralls": "^2.11.16",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.1",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^18.1.0",
    "json-loader": "^0.5.4",
    "jsx-loader": "^0.13.2",
    "moxios": "^0.3.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.4.2",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-polyfill": "^6.22.0",
    "classnames": "^2.2.5",
    "dotenv": "^4.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "redux-promise": "^0.5.3",
    "redux-saga": "^0.14.3",
    "reselect": "^2.5.4",
    "sanitize.css": "^4.1.0",
    "styled-components": "^1.4.3"
  }
}
